padronizar_data <- function(vetor_data){
vetor_data <-  paste0(str_sub(vetor_data,start = 7, end = 10),
str_sub(vetor_data,start = 3, end = 6),
str_sub(vetor_data,start = 1, end = 2))
vetor_data <- str_replace_all(vetor_data,"/", "-")
return(vetor_data)
}
